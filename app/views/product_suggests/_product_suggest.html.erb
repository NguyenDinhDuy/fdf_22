<tr>
  <td> <%= product_suggest.id %> </td>
  <td> <%= product_suggest.name %> </td>
  <td> <%= product_suggest.price %> </td>
  <td> <%= product_suggest.description %> </td>
  <td> <%= image_tag product_suggest.image, class: "image" %></td>
  <td> <%= product_suggest.status %> </td>
  <td>
  <span>
    <%= link_to t("views.product_suggests.index.edit_button"),
      edit_product_suggest_path(product_suggest),
      class:"btn btn-primary" %>
  </span>
  </td>
  <td>
  <span>
    <%= link_to t("views.product_suggests.index.cancel_button"), product_suggest_path(product_suggest),
      method: :delete, data: {confirm: t("views.product_suggests.index.delete_confirm")},
      class:"btn btn-danger" %>
  </span>
  </td>
</tr>
